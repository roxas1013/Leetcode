class Solution:
    def mergeTwoLists(self, l1: ListNode, l2: ListNode) -> ListNode:
        du = ListNode(None)
        x = du
        while (l1 != None) and (l2 != None):
            if l1.val <= l2.val:
                x.next = l1
                l1 = l1.next
            else:
                x.next = l2
                l2 = l2.next
            x = x.next
      
        x.next = l1 or l2
        return du.next

        
